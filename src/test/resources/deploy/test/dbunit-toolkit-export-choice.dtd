<!ELEMENT dataset (
   (ACCESS_POINTS|
    LOG|
    RELATED|
    RESOURCE_MAP|
    RESOURCE_OWNER_HOSTS|
    TASK|
    TODO|
    VERSIONS|
    VOCABULARIES)*)>

<!ELEMENT ACCESS_POINTS EMPTY>
<!ATTLIST ACCESS_POINTS
    ID CDATA #IMPLIED
    PORTAL_DATA CDATA #IMPLIED
    TOOLKIT_DATA CDATA #IMPLIED
    TYPE CDATA #IMPLIED
    VERSION_ID CDATA #IMPLIED
>

<!ELEMENT LOG EMPTY>
<!ATTLIST LOG
    ID CDATA #IMPLIED
    CLASS CDATA #IMPLIED
    MESSAGE CDATA #IMPLIED
    TYPE CDATA #IMPLIED
>

<!ELEMENT RELATED EMPTY>
<!ATTLIST RELATED
    ID CDATA #IMPLIED
    DATA CDATA #IMPLIED
    RELATION CDATA #IMPLIED
    SLUG CDATA #IMPLIED
    TITLE CDATA #IMPLIED
    TYPE CDATA #IMPLIED
>

<!ELEMENT RESOURCE_MAP EMPTY>
<!ATTLIST RESOURCE_MAP
    ID CDATA #IMPLIED
    ACCESS_POINT_ID CDATA #IMPLIED
    DEPRECATED CDATA #IMPLIED
    IRI CDATA #IMPLIED
    OWNED CDATA #IMPLIED
    RESOURCE_TYPE CDATA #IMPLIED
>

<!ELEMENT RESOURCE_OWNER_HOSTS EMPTY>
<!ATTLIST RESOURCE_OWNER_HOSTS
    ID CDATA #IMPLIED
    END_DATE CDATA #IMPLIED
    HOST CDATA #IMPLIED
    OWNER CDATA #IMPLIED
    START_DATE CDATA #IMPLIED
>

<!ELEMENT TASK EMPTY>
<!ATTLIST TASK
    ID CDATA #IMPLIED
    PARAMS CDATA #IMPLIED
    RESPONSE CDATA #IMPLIED
    STATUS CDATA #IMPLIED
    VERSION_ID CDATA #IMPLIED
    VOCABULARY_ID CDATA #IMPLIED
>

<!ELEMENT TODO EMPTY>
<!ATTLIST TODO
    ID CDATA #IMPLIED
    DESCRIPTION CDATA #IMPLIED
    SUMMARY CDATA #IMPLIED
>

<!ELEMENT VERSIONS EMPTY>
<!ATTLIST VERSIONS
    ID CDATA #IMPLIED
    DATA CDATA #IMPLIED
    STATUS CDATA #IMPLIED
    TITLE CDATA #IMPLIED
    VOCAB_ID CDATA #IMPLIED
>

<!ELEMENT VOCABULARIES EMPTY>
<!ATTLIST VOCABULARIES
    ID CDATA #IMPLIED
    CREATED_DATE CDATA #IMPLIED
    DATA CDATA #IMPLIED
    DESCRIPTION CDATA #IMPLIED
    LICENCE CDATA #IMPLIED
    MODIFIED_DATE CDATA #IMPLIED
    MODIFIED_WHO CDATA #IMPLIED
    OWNER CDATA #IMPLIED
    POOL_PARTY_ID CDATA #IMPLIED
    SLUG CDATA #IMPLIED
    STATUS CDATA #IMPLIED
    TITLE CDATA #IMPLIED
    USER_OWNER CDATA #IMPLIED
>

